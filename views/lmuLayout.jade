doctype html
html
  head
    title= title
    script(src="//code.jquery.com/jquery-1.11.0.min.js")

    link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.0/css/bootstrap.min.css")
    link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.0/css/bootstrap-theme.min.css")
    script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.0/js/bootstrap.min.js")

    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    img(src= "/images/splatterTopRight.png" id="top-right-image" alt="paint splash")
    nav(class="navbar navbar-default navbar-fixed-top" role="navigation")
      div(class="container")
        ul(class="nav navbar-nav")
          if title == "Haley Young"
            li(class="nav-link selected")
              a(href="/lmuHome") Home
          else
            li(class="nav-link")
              a(href="/lmuHome") Home

          if title == "Syllabus"
            li(class="nav-link selected")
              a(href="/cmsi185/syllabus") Syllabus
          else
            li(class="nav-link")
              a(href="/cmsi185/syllabus") Syllabus

          if title == "Course Notes"
            li(class="nav-link selected")
              a(href="/cmsi185/courseNotes") Course Notes
          else
            li(class="nav-link")
              a(href="/cmsi185/courseNotes") Course Notes
  block content